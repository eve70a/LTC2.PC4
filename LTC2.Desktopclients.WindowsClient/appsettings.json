{
  "AppSettings": {
    "Name": "Control panel",
    "StartPage": "http://localhost:50000",
    "EnableRefreshFor": "http://localhost:50000/ltc2-main/index.html",
    "MonitoredComponent": "calculator,webapp",
    "PingDeltaInSeconds": 10,

    "WebApp": "..\\LTC2.Webapps.MainApp\\LTC2.Webapps.MainApp.exe",
    "WebAppParameters": "pipe:webapp",
    "WebAppNoWindow": true,
    "WebAppWindowMinimized": false,

    "CalculatorApp": "..\\LTC2.Services.Calculator\\LTC2.Services.Calculator.exe",
    "CalculatorAppParameters": "pipe:calculator",
    "CalculatorNoWindow": true,
    "CalculatorWindowMinimized": false,

    "ProfileApp": "..\\LTC2.Desktopclients.ProfileManager\\LTC2.Desktopclients.ProfileManager.exe",

    "LogFolders": "%LOCALAPPDATA%\\LTC2\\PC4\\Logs\\Calculator,%LOCALAPPDATA%\\LTC2\\PC4\\Logs\\MainApp",
    "WebviewRoot":  "%LOCALAPPDATA%\\LTC2\\PC4\\Misc\\Webview",
    "DisablePasswordSave": false
  },
  "LTC2HttpProxySettings": {
    "Url": "http://localhost:50000",
    "TimeOut": 5000,
    "Pooled": true
  },
  "GenericSettings": {
    "Id": "PC4",
    "Name": "Postcode-gebieden in Nederland",
    "SecretsFolder": "%LOCALAPPDATA%\\LTC2\\PC4\\Secrets"
  }
}